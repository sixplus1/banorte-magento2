<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <columns name="sales_order_columns">

        <column name="resultado_payw" class="Sixplus1\Banorte\Ui\Component\Listing\Column\ResultadopaywColumn">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" xsi:type="string" translate="true">Resultado Payworks</item>
                    <item name="visible" xsi:type="boolean">true</item>
                </item>
            </argument>
        </column>

        <column name="codigo_aut_payw" class="Sixplus1\Banorte\Ui\Component\Listing\Column\CodigoautpaywColumn">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" xsi:type="string" translate="true">Código Autorización Payworks</item>
                    <item name="visible" xsi:type="boolean">true</item>
                </item>
            </argument>
        </column>

        <column name="referencia_payw" class="Sixplus1\Banorte\Ui\Component\Listing\Column\ReferenciapaywColumn">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">int</item>
                    <item name="label" xsi:type="string" translate="true">No. Referencia Payworks</item>
                    <item name="visible" xsi:type="boolean">true</item>
                </item>
            </argument>
        </column>


    </columns>

</listing>